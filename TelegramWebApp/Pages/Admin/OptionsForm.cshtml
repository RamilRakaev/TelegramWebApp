@page
@model TelegramWebApp.Pages.Admin.OptionsFormModel
@using Domain.Model;
@{
    var webAppOptions = new WebAppOptions();
}
<div class="row justify-content-center">
    <form method="post">
        <div class="form-group">
            <label class="col-form-label">ApiKey</label>
            <input class="form-control" asp-for="@webAppOptions.ApiKey" />
        </div>
        <div class="form-group">
            <label class="col-form-label">CalandarId</label>
            <input class="form-control" asp-for="@webAppOptions.CalandarId" />
        </div>
        <div class="form-group">
            <label class="col-form-label">Telegram Bot Token</label>
            <input class="form-control" asp-for="@webAppOptions.Token" />
        </div>
        <button class="btn btn-dark">Отправить</button>
        <a class="btn btn-danger" asp-page="AdminPanel">Назад</a>
    </form>
</div>